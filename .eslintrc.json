{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": "off",
        "new-cap":"off",
        "@typescript-eslint/no-unused-vars": "off",
        "arrow-parens": "off",
        "no-unused-vars": "off"
    }
}
